{
    "manifest_version":2,
    "name":"Detach Site Blocker",
    "description":"Blocks the unwanted sites and boosts your productivity.",
    "version":"1.0",
    "browser_action":{
        "default_popup":"popup.html",
        "default_icon":"icon16.png"
    },
    "icons":{
        "16":"icon16.png",
        "48":"icon48.png",
        "128":"icon128.png"
    },
    "background":{
      "scripts":["eventPage.js"],
      "persistent":false
    },
    "permissions":["storage","tabs"]   
}